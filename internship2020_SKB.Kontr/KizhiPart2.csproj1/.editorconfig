[*.cs]

# SA1503: Braces should not be omitted
dotnet_diagnostic.SA1503.severity = silent

# SA1101: Prefix local calls with this
dotnet_diagnostic.SA1101.severity = silent

# SA1600: Elements should be documented
dotnet_diagnostic.SA1600.severity = silent

# CA1812: Interpreter.TestInterpreter is an internal class that is apparently never instantiated. If so, remove the code from the assembly. If this class is intended to contain only static members, make it static (Shared in Visual Basic).
dotnet_diagnostic.CA1812.severity = none

# CA1016: Mark assemblies with assembly version
dotnet_diagnostic.CA1016.severity = none

# CA1822: Member CheckCountOfArgs does not access instance data and can be marked as static (Shared in VisualBasic)
dotnet_diagnostic.CA1822.severity = none

# SA1633: File should have header
dotnet_diagnostic.SA1633.severity = none

# IDE0022: Use block body for methods
csharp_style_expression_bodied_methods = false:silent
